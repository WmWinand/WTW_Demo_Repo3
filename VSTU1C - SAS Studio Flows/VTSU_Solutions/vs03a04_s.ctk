            <Option decimalPlaces="0" defaultValue="640" increment="1" inputType="numstepper" invalidMessage="Enter an integer from 50 to 1200 for the width in pixels." maxValue="1200" minValue="50" missingMessage="Enter an integer from 50 to 1200 for the width in pixels." name="pixelWidth" rangeMessage="Enter an integer from 50 to 1200 for the width in pixels." required="true">Width:</Option>
            <Option decimalPlaces="0" defaultValue="480" increment="1" inputType="numstepper" invalidMessage="Enter an integer from 50 to 1200 for the height in pixels." maxValue="1200" minValue="50" missingMessage="Enter an integer from 50 to 1200 for the height in pixels." name="pixelHeight" rangeMessage="Enter an integer from 50 to 1200 for the height in pixels." required="true">Height:</Option>
