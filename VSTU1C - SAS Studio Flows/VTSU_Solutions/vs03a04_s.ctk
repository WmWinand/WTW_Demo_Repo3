            <Option defaultValue="16" increment="0.1" inputType="numstepper" invalidMessage="Enter a number from 5 to 30 for the width in cms." maxValue="30" minValue="5" missingMessage="Enter a number from 5 to 30 for the width in cms." name="cmWidth" rangeMessage="Enter a number from 5 to 30 for the width in cms." required="true">Width:</Option>
